package Fundamentos;

public class Temperatura {
	
	public static void main(String[] args) {
		// (F° - 32) * 5/9 = C°
		final double ajuste = 32;
		final double multiplicador = 0.5555555555555556;
		
		double F = 100;
		
		double C = (F-ajuste) * multiplicador;
		
		System.out.println("São "+ C +"° Celsius");
	}

}
