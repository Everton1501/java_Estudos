package teste;

public class Teste {

	public static void main(String[] args) {
		
		int idade = 15;
		boolean  amigoDoDono= true;
		
		if(idade < 18 && amigoDoDono == false ) {
			System.out.println("Não pode entrar");
		} else {
			System.out.println("Pode entrar");
		}
		
		if (idade < 18 || !amigoDoDono);{
			System.out.println("Não pode entrar");
		} 
	
		for (int i = 0; i < 100; i++) {
		    if (i > 50 && i < 60) {
		        break;
		    }
		    System.out.println(i);
		}
		
	}
}