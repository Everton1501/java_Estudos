package oo.heranca.desafio;

public class Sistema {
	
	public static void main(String[] args) {
		
		Ferrari ferrari = new Ferrari();
		Civic civic = new Civic();
		
		ferrari.velocidadeAtual = 100;
		ferrari.acelerar();
		ferrari.acelerar();
		ferrari.acelerar();
		ferrari.frear();		
		System.out.println(ferrari.velocidadeAtual);
		
		
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		civic.frear();		
		civic.frear();		
		civic.frear();		
		civic.frear();		
		civic.frear();		
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		civic.acelerar();
		System.out.println(civic.velocidadeAtual);
	}
}
